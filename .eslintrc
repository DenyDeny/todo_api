{
    "rules": {
      "quotes": [2, "single", "avoid-escape"],
      "eqeqeq": 0,
      "brace-style": [2, "1tbs"],
      "eol-last": 0,
      "no-nested-ternary": 1,
      "padded-blocks": [1, "never"],
      "space-before-function-paren": [
        1,
        "always"
      ],
      "space-before-blocks": [1, "always"],
      "arrow-body-style": [
        "error",
        "always"
      ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
